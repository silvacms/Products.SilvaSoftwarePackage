<div class="silva-software-group">
  <table class="silvatable list">
    <thead>
      <tr>
        <th class="align-left"
            tal:content="context/get_title">Group Name</th>
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="package view/get_packages">
        <td>
          <a tal:attributes="href package/url"
             tal:content="package/name">
          </a>
        </td>
      </tr>
    </tbody>
  </table>
</div>
